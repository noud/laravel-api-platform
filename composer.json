{
    "name": "noud/laravel-api-platform",
    "require": {
        "php": ">=7.2.5",
        "appointer/swaggervel": "dev-develop-8.x",
        "doctrine/dbal": "~2.3",
        "doctrine/inflector": "dev-dutch",
        "fruitcake/laravel-cors": "dev-develop-8.x",
        "infyomlabs/laravel-generator": "dev-develop-8.x",
        "infyomlabs/swagger-generator": "dev-master"
    },
    "description": "Laravel package conform API-Platform",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Noud de Brouwer",
            "email": "noud4@home.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "API\\Platform\\": "src/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "API\\Platform\\Providers\\APIPlatformServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../inflector"
        },
        {
            "type": "path",
            "url": "../flash"
        },
        {
            "type": "path",
            "url": "../laravel-cors"
        },
        {
            "type": "path",
            "url": "../laravel-generator"
        },
        {
            "type": "path",
            "url": "../swaggervel"
        }
    ]
}